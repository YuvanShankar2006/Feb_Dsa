class Solution {
    public int searchInsert(int[] nums, int target) {
        if(target==nums[nums.length-1]){
            return nums.length-1;
        }
        else if(nums[0]>=target){
            return 0;
        }
        else{
            for(int i=1;i<=nums.length-1;i++){
                if(nums[i]>=target){
                    return i;
                }
            }
        }
        return nums.length;
    }
}
