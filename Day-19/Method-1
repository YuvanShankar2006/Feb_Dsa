class Solution {
    public int[] searchRange(int[] nums, int target) {
    int n=nums.length;
    int j=n-1;
    int i=0;
    int[]ans=new int[]{-1,-1};
    while(i<n){
        if(nums[i]==target){
            ans[0]=i;
            break;
        }
        i++;
    }
    while(j>=0){
        if(nums[j]==target){
            ans[1]=j;
            break;
        }
        j--;
    }
    return ans;
    }
}
