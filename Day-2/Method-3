class Solution {
    public int majorityElement(int[] nums) {
        int el=nums[0];
        int curr=1;
        for(int i=1;i<nums.length;i++){
            if(nums[i]==el){
                curr++;
            }
            else{
                curr--;
                if(curr==0){
                    el=nums[i];
                    curr=1;
                }
            }
        }
        return el;
    }
}
