class Solution {
    public boolean isPalindrome(String s) {
        String result = s.replaceAll("[^a-zA-Z0-9]", "").toLowerCase();
        System.out.print(result);
        int j=result.length()-1;
        for(int i=0;i<result.length()/2;i++){
            if(result.charAt(i)!=result.charAt(j)){
                return false;
            }
            j--;
        }
        return true;
    }
}
