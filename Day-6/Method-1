class Solution {
    public int findKthPositive(int[] arr, int k) {
        
        int[] present = new int[2001];

        for (int x : arr) {
            present[x] = 1;
        }

        for (int i = 1; i <= 2000; i++) {
            if (present[i] == 0) {
                k--;
                if (k == 0) return i;
            }
        }
        return -1; 
    }
}
