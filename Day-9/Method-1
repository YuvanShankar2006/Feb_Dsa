class Solution {
    public int[] plusOne(int[] digits) {
        int size=digits.length;
        for(int i=digits.length-1;i>=0;i--){
            if(digits[i]<9){
               digits[i]++;
               return digits;
            }
        digits[i]=0;
        }
        int[] newlist = new int[size + 1];
        newlist[0]=1;
        return newlist;
    }
}
